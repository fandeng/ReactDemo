{
    "html": "<!DOCTYPE html><html><head lang='en'><meta charset='utf-8'><meta name='viewport' content='width=device-width, initial-scale=1'><meta http-equiv='X-UA-Compatible' content='IE=edge'> <style type='text/css'>* { margin: 0;padding: 0;box-sizing: border-box;}
